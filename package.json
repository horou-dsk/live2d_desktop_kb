{
  "name": "dango",
  "version": "0.1.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^22.1.0"
  },
  "build": {
    "appId": "com.dango.live2desktop",
    "icon": "ico/dango.ico",
    "productName": "団子",
    "extraResources": [
      "./live2d_models"
    ]
  },
  "dependencies": {
    "iohook": "^0.6.1"
  },
  "iohook": {
    "targets": [
      "node-72",
      "electron-73"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
